<%= render 'shared/page_title', title: "Log In" %>
	
<div class="row">
	<div class='well col-md-6 col-md-offset-3'>
		<%= form_tag '/login' do %>

			<%= label_tag :email %>
			<%= text_field_tag :email %> <br/><br/>

			<%= label_tag :password %>
			<%= password_field_tag :password %> <br/><br/>

			<%= submit_tag 'Log In', class: 'btn btn-success' %>

		<% end %>
	</div>
</div>
	


	

